{
  "name": "kasi-listings-app",
  "version": "1.0.0",
  "description": "Comprehensive township listing app for jobs, rentals, skills, and business services",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "township",
    "listings",
    "rentals",
    "jobs",
    "skills",
    "business",
    "marketplace"
  ],
  "author": "Kasi Listings Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "multer": "^1.4.5-lts.1",
    "cloudinary": "^1.40.0",
    "stripe": "^13.5.0",
    "nodemailer": "^6.9.4",
    "express-validator": "^7.0.1",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "express-session": "^1.17.3",
    "connect-mongo": "^5.0.0",
    "socket.io": "^4.7.2",
    "redis": "^4.6.8",
    "sharp": "^0.32.5",
    "uuid": "^9.0.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0",
    "prettier": "^3.0.1",
    "@types/node": "^20.5.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
} 